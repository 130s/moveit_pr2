<launch>

  <node pkg="benchmark_manipulation_tests" name="benchmark_manipulation_tests" type="runTest" output="screen" >
    <rosparam command="load" file="$(find benchmark_manipulation_tests)/config/single_arm.yaml" />
    <rosparam command="load" file="$(find benchmark_manipulation_tests)/config/locations.yaml" />
    <rosparam command="load" file="$(find benchmark_manipulation_tests)/config/experiments.yaml" />
    <param name="known_objects_filename" value="$(find benchmark_manipulation_tests)/env/tabletop.env" />

    <param name="benchmark_results_folder" value="$(find benchmark_manipulation_tests)/results/" />
  </node>

</launch>
